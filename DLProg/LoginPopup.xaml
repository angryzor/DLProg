<Window x:Class="DLProg.LoginPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoginPopup" Height="532" Width="767">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="48" />
        </Grid.RowDefinitions>
        <Button Content="OK" Grid.Row="1" Height="23" Margin="0,12,12,12" Name="okButton" VerticalAlignment="Center" Width="75" HorizontalAlignment="Right" Click="okButton_Click" />
        <WebBrowser Name="loginPage" />
    </Grid>
</Window>
